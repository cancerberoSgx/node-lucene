{
  "name": "javap",
  "version": "0.0.1",
  "description": "Given a .jar and class name it prints the Java AST in JSON format. A facade for javap-json with nice cli and node.js API.",
  "main": "dist/src/index.js",
  "bin": "bin/javap-parser.js",
  "bindings": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "prepare": "npm run build",
    "test": "npm run build && jest"
  },
  "keywords": [
    "java",
    "javascript",
    "typescript",
    "node"
  ],
  "author": "Sebastian Gurin",
  "repository": "https://github.com/cancerberoSgx/javap",
  "license": "MIT",
  "dependencies": {
    "get-stdin": "^6.0.0",
    "java": "^0.10.0",
    "minimatch": "^3.0.4",
    "minimist": "^1.2.0",
    "shelljs": "^0.8.2"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@types/minimatch": "^3.0.3",
    "@types/minimist": "^1.2.0",
    "@types/node": "^10.3.6",
    "@types/shelljs": "^0.8.0",
    "jest": "^23.3.0",
    "ts-jest": "^23.0.0",
    "ts-loader": "^4.4.2",
    "ts-node": "^6.1.0",
    "typescript": "2.9.2"
  }
}
