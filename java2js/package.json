{
  "name": "java2js",
  "version": "0.0.1",
  "description": "Generates TypeScript interfaces of given Java class names and .jars as a project structure in FS. Also, generate JavaScript implementations of given Java classes of .jars / .class files based on node-java and optionally TypeScript interfaces / classes",
  "main": "dist/src/index.js",
  "bindings": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "prepare": "npm run build",
    "test": "jest"
  },
  "keywords": [
    "java",
    "javascript",
    "typescript",
    "node"
  ],
  "author": "Sebastian Gurin",
  "repository": "https://github.com/cancerberoSgx/java2js",
  "license": "MIT",
  "dependencies": {
    "java": "^0.10.0",
    "javap": "file:../javap"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.3.6",
    "typescript": "2.9.2",
    "jasmine": "^3.1.0",
    "jest": "^23.3.0",
    "ts-jest": "^23.0.0",
    "ts-loader": "^4.4.2",
    "ts-node": "^6.1.0"
  }
}
