{
  "name": "JavapJsonOutput",
  "version": "0.0.1",
  "description": "java program like javap which prints output in json. Uses node small cli. ",
  "keywords": [
    "lucene",
    "search",
    "index"
  ],
  "author": "Sebastian Gurin",
  "license": "MIT",
  "scripts": {
    "build": "npm run java-build",
    "java-build": "javac -cp \"java-lib/gson-2.8.5.jar\" java-src/JavapJsonOutput.java",
    "java-test1": "npm run java-build && java -cp \"java-lib/gson-2.8.5.jar:java-src\" JavapJsonOutput java.lang.String,java.util.List",
    "java-test2": "npm run java-build && java -cp \"java-lib/gson-2.8.5.jar:java-src\" JavapJsonOutput org.apache.lucene.store.RAMDirectory,org.apache.lucene.store.LockFactory,org.apache.lucene.util.Accountable,org.apache.lucene.util.FilterIterator ../node-lucene/lucene-lib/lucene-core-7.4.0.jar"
  },
  "repository": "https://github.com/cancerberoSgx/lucene-java",
  "devDependencies": {},
  "dependencies": {
    "yamat": "0.0.14"
  }
}
