  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "javap test",
      "cwd": "${workspaceFolder}/javap",
      // "runtimeExecutable": "ts-node",
      "program": "${workspaceFolder}/javap/node_modules/ts-node/dist/bin.js", 
      "args": [
        "node_modules/jasmine/bin/jasmine.js",
        "--config=src/__tests__/jasmine.json"
      ]
      // "preLaunchTask": "javap:build",
      
    },
    {
      "type": "node",
      "request": "launch",
      "name": "java2js test",
      "cwd": "${workspaceFolder}/java2js",
      "program": "${workspaceFolder}/java2js/src/__tests__/index.ts", 
      "outFiles": [
        "${workspaceFolder}/java2js/dist/**/*"
      ],
      "args": [
      ],
      "preLaunchTask": "java2js:build"
    }
  ]
}